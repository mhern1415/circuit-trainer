<%= render 'layouts/jumbotron' %>

<div class="container">
<br>
<h1 class="text-primary">Log In</h1>
<br>
<% if flash[:message]%>
  <%= flash[:message] %>
<% end %>
<%= form_for :user do |f| %>

    <%= f.label :username %>
    <%= f.text_field :username %>
    <br><br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br><br>
      <%= f.submit "Log In" %>
  <% end %>
  </div>