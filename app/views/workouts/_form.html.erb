<%= form_for @workout do |f| %>

    <%= f.label :title %>
    <%= f.text_field :title %>
    <br></br>
  
    <%= f.label :circuit_count, "How many circuits do plan on adding?" %>
    <%= f.number_field :circuit_count, min: 0 %>
    <br></br>
  
    <%= f.label :description %>
    <%= f.text_field :description %>
  
      <br></br>
  
    <%= f.submit%>
  
  <% end %>