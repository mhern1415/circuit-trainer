<h1>Your Workouts</h1>

<% @workouts.each do |workout|%>
  <div>
    <h3><%= link_to workout.title, workout_path(workout) %> </h3>
    <p>Type: <%= workout.description %> |
    Circuit Count: <%= workout.circuit_count %> </p>
    <%= link_to 'Circuits in this Workout', workout_circuits_path(workout) %>
    <%= workout.circuits.size %>
  </div><br>
<% end %>

